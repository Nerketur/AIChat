<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="TestRouterMvvm.App"
             xmlns:local="using:TestRouterMvvm"
             xmlns:settings="using:TestRouterMvvm.Views.Settings"
             xmlns:vmsettings="using:TestRouterMvvm.ViewModels.Settings"
             xmlns:viewmodels="using:TestRouterMvvm.ViewModels"
             xmlns:views="using:TestRouterMvvm.Views"
             xmlns:dialogHostAvalonia="clr-namespace:DialogHostAvalonia;assembly=DialogHost.Avalonia"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <!--<local:ViewLocator/>-->
        <DataTemplate DataType="{x:Type viewmodels:HomepageViewModel}">
            <views:HomepageView />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vmsettings:ImportExportSettingsViewModel}">
            <settings:ImportExportSettingsView />
        </DataTemplate>
    </Application.DataTemplates>  
    <Application.Styles>
        <FluentTheme />
        <dialogHostAvalonia:DialogHostStyles />
    </Application.Styles>

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <MergeResourceInclude Source="/Assets/ResourceDictionary1.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

</Application>