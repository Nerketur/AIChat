<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:views="using:TestRouterMvvm.Views.Settings"
             xmlns:viewmodels="using:TestRouterMvvm.ViewModels"
             xmlns:settingsviewmodels="using:TestRouterMvvm.ViewModels.Settings"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="viewmodels:SettingsViewModel"
             x:Class="TestRouterMvvm.Views.Routed.SettingsView">
    <Design.DataContext>
        <viewmodels:SettingsViewModel/>
    </Design.DataContext>
    <TabControl>
        <TabControl.DataTemplates>

            <DataTemplate DataType="{x:Type settingsviewmodels:GeneralSettingsViewModel}">
                <!--<ContentControl Content="{Binding }" />-->
                <Button Content="This is the General Settings View in the DataTemplate" HorizontalAlignment="Center" VerticalAlignment="Center"/>
            </DataTemplate>
            <!--<DataTemplate DataType="{x:Type settingsviewmodels:AdvancedSettingsViewModel}">
                <ContentControl Content="{Binding }" />
            </DataTemplate>-->
	
        </TabControl.DataTemplates>
        <TabItem Header="{Binding Tab1VM.DisplayName}">
            <views:ImportExportSettingsView DataContext="{Binding Tab1VM}" />
            <!--<ContentControl Content="{Binding }" />-->
        </TabItem>
        <TabItem Header="{Binding Tab2VM.DisplayName}">
            <settingsviewmodels:GeneralSettingsViewModel />
            <!--<views:GeneralSettingsView DataContext="{Binding Tab2VM}" />-->
        </TabItem>
        <TabItem Header="{Binding Tab3VM.DisplayName}">
            <settingsviewmodels:AdvancedSettingsViewModel />
            <!--<views:AdvancedSettingsView DataContext="{Binding Tab3VM}" />-->
        </TabItem>
    </TabControl>
</UserControl>
